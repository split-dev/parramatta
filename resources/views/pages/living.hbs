<!DOCTYPE html>
<html lang="en">
<head>
  {{> ../common/head }}
</head>
<body class="living">
{{> ../common/mega-menu }}
{{#with topInfo}}
  <div class="category-bg">
    <div class="category-bg__info" style="background-image: url(' {{image}} ')"></div>
    <div class="category-bg__box">
      <h3>{{category}}</h3>
      <h1>{{title}}</h1>
    </div>
  </div>
{{/with}}
<section class="content">
  <div class="row">
    <div class="small-12 medium-7 large-6 large-offset-1 column">
      <h2>Submit your event ipsum dolor sit amet</h2>
      <span class="link">Before you begin, please read our <a href="#">Event Guidelines.</a></span>
      <h3>Event Information</h3>
      <form class="form">
        <span>Text field</span>
        <input type="text">
        <span>Event type</span>
        <div class="select">
          <input type="radio" id="radio1" name="radio1"/>
          <label for="radio1">One off event</label>
        </div>
        <div class="select">
          <input type="radio" id="radio2" name="radio1"/>
          <label for="radio2">Repeating event</label>
        </div>
        <span>Start date</span>
        <div class="date">
          <input type='text' class='datepicker-start' data-date-format="d  /  MM  /  yyyy" placeholder="Day  /  Month  /  Year"/>
